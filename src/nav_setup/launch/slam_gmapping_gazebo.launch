<?xml version="1.0"?>

<launch>

    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
      <rosparam>
      odom_frame: odom
      map_update_interval: 1.0
      maxUrange: 15.0
      maxRange: 20.0
      sigma: 0.05
      kernelSize: 1
      lstep: 0.05
      astep: 0.05
      iterations: 5
      lsigma: 0.075
      ogain: 3.0
      lskip: 0
      linearUpdate: 0.1
      angularUpdate: 0.1
      temporalUpdate: 0.5
      resampleThreshold: 0.1
      particles: 30
      xmin: -1.0
      ymin: -1.0
      xmax: 1.0
      ymax: 1.0
      delta: 0.025
      llsamplerange: 0.01
      llsamplestep: 0.01
      lasamplerange: 0.005
      lasamplestep: 0.005
      base_frame: base_link
    </rosparam>
    </node>

	    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find nav_setup)/rviz_config/attempt1.rviz" />

</launch>
