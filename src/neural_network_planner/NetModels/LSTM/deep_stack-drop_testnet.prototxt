layer {
  name: "lstm6"
  type: "LSTM"
  bottom: "drop5"
  bottom: "clip"
  top: "lstm6"
  recurrent_param {
    num_output: 64
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
  name: "drop6"
